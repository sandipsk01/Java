package Patterns;

import java.util.Scanner;

public class OddSquarePattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int i=1;
		while(i<=n*2) {
			if(i%2==1) {
				System.out.print(i);
				}
			}
			System.out.println();
			i=i+1;
		}
	}

}
