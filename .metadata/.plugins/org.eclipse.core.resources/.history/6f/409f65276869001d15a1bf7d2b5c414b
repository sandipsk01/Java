package fundamentals;
import java.util.Scanner;
public class ForLoopFibo {
	public static void main(String[] args) {
		Scanner s= new Scanner(System.in);
		int n=s.nextInt();
		int a = 0, b = 1, c;
		if(n == 0) {
			System.out.print(a);
			return;
		}
		for(int i = 2; i <= n; i++){
			c = a + b;
			a = b;
			b = c;
		}
		System.out.print(b);
		return;
		
	}
}
