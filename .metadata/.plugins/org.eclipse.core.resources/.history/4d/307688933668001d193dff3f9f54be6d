package Patterns;

import java.util.Scanner;

public class IsosclesTriangle {
	public static void main(String[]args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int i=1;
		while(i<=n) {
			int spaces=1;
			while(spaces<=n-i) {
				System.out.print(' ');
				spaces=spaces+1;
			}
			int inc=1;
			while(inc<=i) {
				System.out.print(inc);
				inc=inc+1;
			}
			
		}
		System.out.println();
		i=i+1;
	}

}
