package Patterns;

import java.util.Scanner;

public class OddSquarePattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int i=1;
		while(i<=n*2) {
			if(i%2==1) {
				int j=i;
				while(j<=n-1) {
					if(j%2==1) {
						System.out.print(j);
						j=j+1;
					}
				}
			}
			i=i+1;
		}
	}

}
