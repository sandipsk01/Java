package ArrayList;

import java.util.ArrayList;

public class ArrayListDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList <Integer> arrlist= new ArrayList(11);   //ArrayList used to work on dynamic array
		arrlist.add(10);
		System.out.println(arrlist.size());   // no. of elements filled  
		// default capacity of ArrayList in java is 10 and it gets increase generally by 1.5
		arrlist.add(20);
		arrlist.add(0,30);
		arrlist.set(0,5);
		arrlist.remove(2);
		Integer i=2;
		arrlist.remove(2);
	}

}
