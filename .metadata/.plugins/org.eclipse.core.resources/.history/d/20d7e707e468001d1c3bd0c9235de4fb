package Patterns;
import java.util.Scanner;
public class SumPattern {
	public static void main(String[]args) {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int i=1;
		while(i<=n) {
		    int sum=0;
			int nums=1;
			while(nums<=i) {
				if(i==1) {
					System.out.print(nums);
					System.out.print('=');
					System.out.print(nums);
				}
				else if(nums==i){
					System.out.print(nums);
					System.out.print('=');
					System.out.print(sum);
				}
				else{
				    System.out.print(nums);
				    System.out.print('+');	    
				}
				sum=sum+nums;
				nums=nums+1;
			}
			System.out.println();
			i=i+1;
		}
	}
}
